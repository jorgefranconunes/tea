class TNode (

    _nextSibling
)





###########################################################################
#
# 
#
###########################################################################

method TNode constructor () {
}





###########################################################################
#
# 
#
###########################################################################

method TNode setNextSibling ( node ) {
    
    set! _nextSibling $node
}





###########################################################################
#
# 
#
###########################################################################

method TNode getNextSibling () {
    
    is $_nextSibling
}





