#
# Test for "str-split" function implemented with the GNU regex library.
#
# Args: string separator
#

if { != [length $argv] 2 } {
    echo "Args: string separator"
    exit 0
}

map [lambda (s) { echo "'" $s "'" }] [apply str-split $argv]

