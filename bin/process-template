#!/bin/bash
###########################################################################
#
# Copyright (c) 2008 PDM&FC, All Rights Reserved.
#
###########################################################################

###########################################################################
#
# $Id$
#
#
# Performs the processing of a template file, using the parameters
# defined in the "config/col-develop.conf" config file.
#
# This utility is intended to be used only in the development
# environment.
#
#
# Revisions:
#
# 2008/04/18 Created. (TSK-PDMFC-TEA-0044) (jfn)
#
#########################################################################

_scriptDir=$(dirname $0)
_scriptName=$(basename $0)

_teaBaseDir=$(cd $_scriptDir/..; pwd)

#
# Make utility functions available
#
source ${_teaBaseDir}/bin/tea-utils





#########################################################################
#
# Script main body
#
#########################################################################

if [ $# -lt 1 ] ; then
    colError "Args: template-file [module-name ...]"
fi

source ${_teaBaseDir}/config/tea-core.conf

templateFile=$1

teaParseTemplate $templateFile





#########################################################################
#
# 
#
#########################################################################

